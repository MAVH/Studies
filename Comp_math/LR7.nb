Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"findXFull", "[", 
    RowBox[{"p_", ",", "g_", ",", "h_"}], "]"}], " ", ":=", 
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", " ", "=", " ", "0"}], ",", " ", 
       RowBox[{"i", "\[LessEqual]", " ", 
        RowBox[{"p", "-", "1"}]}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"h", "\[Equal]", 
           RowBox[{"Mod", "[", 
            RowBox[{
             SuperscriptBox["g", "i"], ",", "p"}], "]"}]}], ",", 
          RowBox[{"Break", "[", "]"}]}], "]"}], ";"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"i", "\[Equal]", "p"}], ",", " ", 
       RowBox[{"Print", "[", "\"\<No results\>\"", "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "i", "]"}], ";"}], "\[IndentingNewLine]", ")"}]}],
   ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"findXStep", "[", 
    RowBox[{"p_", ",", "a_", ",", "b_"}], "]"}], " ", ":=", 
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{"u", ";", "v", ";", "\[IndentingNewLine]", 
     RowBox[{"hh", " ", "=", " ", 
      RowBox[{
       RowBox[{"Floor", "[", 
        RowBox[{"Sqrt", "[", "p", "]"}], "]"}], " ", "+", "1"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"cc", " ", "=", " ", 
      RowBox[{"Mod", "[", 
       RowBox[{
        SuperscriptBox["a", "hh"], ",", " ", "p"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"uu", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"{", "}"}], ",", 
        RowBox[{"{", "hh", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"vv", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"{", "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"hh", "+", "1"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "<=", "hh"}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"uu", "[", 
          RowBox[{"[", "i", "]"}], "]"}], "=", 
         RowBox[{"Mod", "[", 
          RowBox[{
           SuperscriptBox["cc", "i"], ",", "p"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
